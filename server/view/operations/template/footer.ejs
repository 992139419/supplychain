<script>
    $(function () {
        function menuStatus(parentId, childId) {
            $("#" + parentId).addClass('active');
            $("#a" + parentId).trigger('click');
            $("#" + childId).addClass('active');
        }
        menuStatus('<%=parentId%>', '<%=childId%>');
    });
</script>
